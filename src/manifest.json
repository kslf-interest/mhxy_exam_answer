{
    "manifest_version": 2,
    "name": "梦幻西游手游版 — 科举答案",
    "version": "1.0.0",
    "description": "梦幻西游手游版 — 科举答案 by Leo Wang",
    "icons": {
        "16": "src/resource/images/icon.png",
        "48": "src/resource/images/icon.png",
        "128": "src/resource/images/icon.png"
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "browser_action": {
        "default_icon": {
            "19": "src/resource/images/icon.png",
            "38": "src/resource/images/icon.png"
        },
        "default_title": "梦幻西游手游版 — 科举答案 by Leo Wang",
        "default_popup": "popup.html"
    },
    "permissions": [
        "storage",
        "webRequest",
        "webRequestBlocking",
        "*://fengchao.baidu.com/",
        "*://fctest.baidu.com/",
        "*://fclog.baidu.com/"
    ]
}